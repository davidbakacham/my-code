// index.js

const tasks = [];

function addTask(task) {
  tasks.push(task);
  console.log(`Task "${task}" added.`);
}

function listTasks() {
  console.log("Tasks:");
  tasks.forEach((task, index) => {
    console.log(`${index + 1}. ${task}`);
  });
}

// Додаємо кілька завдань
addTask("Learn JavaScript");
addTask("Write blog post");

listTasks();
